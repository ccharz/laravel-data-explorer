{
    "name": "ccharz/laravel-data-explorer",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Christian Charzewski",
            "email": "chris@cw-software.at",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ccharz\\DataExplorer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ccharz\\DataExplorer\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.3",
        "laravel/prompts": "^0.3",
        "joetannenbaum/chewie": "^0.1",
        "illuminate/support": "^11.0||^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.20",
        "orchestra/testbench": "^9.0||^10.0",
        "rector/rector": "^2.0",
        "larastan/larastan": "^3.0",
        "spatie/phpunit-snapshot-assertions": "^5.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ccharz\\DataExplorer\\DataExplorerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}